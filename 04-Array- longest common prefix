#include <stdio.h>
#include <string.h>

#define MAX 1000

// You will implement this
char* longestCommonPrefix(char arr[][MAX], int n)
{
    int k=0;
    static char lCP[1000];
    for(int i=0;arr[0][i] != '\0';i++)
    {
        int match =0;
        char current = arr[0][i];
        for(int j=1;j<=n-1;j++)
        {
            if(current == arr[j][i])
            {
                match+= 1;
            }
            else
            {
                break;
            }
        }
        if(match == n-1)
        {
            lCP[k++]=current;
        }
        else
        {
            break;
        }
    }
    lCP[k]='\0';
    return lCP;
}

int main()
{
    int n;
    scanf("%d", &n);

    char arr[n][MAX];

    // Read strings
    for(int i = 0; i < n; i++)
    {
        scanf("%s", arr[i]);
    }

    // Call your function
    char* result = longestCommonPrefix(arr, n);

    // Print result
    printf("%s\n", result);

    return 0;
}
