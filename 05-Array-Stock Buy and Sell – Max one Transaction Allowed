#include <stdio.h>

int maxProfit(int prices[], int n)   // your function declaration
{
    int max=0;
    int min=prices[0];
    int profit;
    for(int i=0;i<=n-1;i++)
    {
        if(min >= prices[i])
        {
            min = prices[i];
        }
        else
        {
            profit = prices[i] - min;
            if(max < profit)
            {
                max = profit;
            }
        }
            
    }
    
    return max;
}

int main()
{
    int n;
    
    printf("Enter number of days: ");
    scanf("%d", &n);

    int prices[n];

    printf("Enter stock prices:\n");
    for(int i = 0; i < n; i++)
    {
        scanf("%d", &prices[i]);
    }

    int result = maxProfit(prices, n);

    printf("Maximum Profit: %d\n", result);

    return 0;
}
