#include <stdio.h>
#include <stdbool.h>

bool twoSum(int arr[], int n, int target)  // function declaration
{
    for(int i=0;i<=n-1;i++)
    {
        for(int j=i+1;j<=n-1;j++)
        {
            int sum = arr[i]+arr[j];
            if(sum==target)
            {
                return true;
            }
        }
    }
    return false;
}
int main()
{
    int arr[] = {11};
    int n = sizeof(arr) / sizeof(arr[0]);
    int target = 11;

    if(twoSum(arr, n, target))
        printf("true\n");
    else
        printf("false\n");

    return 0;
}
