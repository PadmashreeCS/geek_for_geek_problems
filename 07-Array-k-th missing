// Online C compiler to run C program online
#include <stdio.h>


int KthMissingElement(int arr[], int n, int k)
{
    int j=arr[0];
    int missing_count = 0;
   for(int i=0;i<=n-1;i++)
   {
      if(arr[i]!=j)
      {
          missing_count += 1;
          i=i-1;
         if(missing_count == k)
         {
             return j;
         }
      }
       j++;
   }
   return -1;
}

int main()
{
    int n, k;

    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter elements in increasing order:\n");
    for(int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }

    printf("Enter value of k: ");
    scanf("%d", &k);

    int result = KthMissingElement(arr, n, k);

    printf("Output: %d\n", result);

    return 0;
}
